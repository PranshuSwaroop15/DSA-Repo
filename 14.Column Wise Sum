#include<iostream>
using namespace std;


int main(){

  int m, n;

  cin >> m >> n;
  int arr[m][n];

  for (int i = 0; i < m; i++) {

    for (int j = 0; j < n; j++) {

      cin >> arr[i][j];
    }

    cout << endl;
  }

  int sum;
  for (int i = 0; i < n; i++) {
    sum = 0;
    for (int j = 0; j < m; j++) {

      // cout<<arr[i][j]<<' ';

      sum += arr[j][i];
    }
    cout << sum << ' ';
  }
}


