
int len(char *input){

    int count=0;

    for(int i=0;input[i]!='\0';i++){
        count++;
    }

    return count;
}
void trimSpaces(char input[]) {
    int count = 0;
    int n=len(input);
    for (int i = 0; i<n; i++)
    if (input[i]!= ' ')
    input[count++] = input[i]; // here count is
    // incremented
    input[count] = '\0';
}
